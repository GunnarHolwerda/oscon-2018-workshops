{
  "name": "OSCON",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all test:find --parallel server build:watch browser",
    "server": "node --experimental-modules index.mjs",
    "build": "webpack",
    "build:watch": "watch \"npm run build\" ./client",
    "browser": "open http:/localhost:8080/ && open http:/localhost:8080/test/",
    "test:find": "find actions client reducers server utils -name '*.spec.js' | sed -En \"s/(.*)/import '..\\/\\1';/p\" > test/all-tests.js",
    "test": "node_modules/.bin/mocha -R nyan --recursive --compilers js:babel-register $(find actions client reducers server utils -name '*.spec.js')"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "ws": "^5.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "deep-freeze": "0.0.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.21.0",
    "watch": "^1.0.2",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  }
}
